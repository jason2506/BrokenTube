{
    "name": "MyTube",
    "version": "1.0.1",
    "description": "Google Chrome extension that adds a button to YouTube page and makes downloading the videos easier.",
    "icons": {
        "128": "icon_128.png",
         "48": "icon_48.png",
         "16": "icon_16.png"
    },
    "content_scripts": [{
        "matches": ["http://www.youtube.com/watch*"],
        "js": [
            "jquery.js",
            "contentscript.js"
        ]
    }]
}

