{
    "name": "BrokenTube",
    "version": "1.2.1",
    "manifest_version": 2,
    "description": "Google Chrome extension that adds a button to YouTube page and makes downloading the videos easier.",
    "update_url": "https://raw.github.com/jason2506/BrokenTube/master/updates.xml",
    "icons": {
        "128": "icons/icon_128.png",
         "48": "icons/icon_48.png",
         "16": "icons/icon_16.png"
    },
    "content_scripts": [{
        "matches": ["http://www.youtube.com/watch*"],
        "js": [
            "jquery.js",
            "contentscript.js"
        ]
    }]
}

